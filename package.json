{
  "name": "collabornest",
  "version": "0.2.1",
  "description": "Real-time collaboration platform for healthcare with distributed locking, WebSocket gateway, and resource management",
  "author": "Antonio Cittadino",
  "private": false,
  "license": "AGPL-3.0",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:coverage:check": "jest --coverage --passWithNoTests --silent",
    "test:tdd": "jest --watch --coverage --verbose",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest.e2e.config.js",
    "test:e2e:safe": "bash scripts/test-env-guard.sh e2e",
    "test:integration": "jest --config ./jest.integration.config.js",
    "test:integration:safe": "bash scripts/test-env-guard.sh integration",
    "test:bdd": "node scripts/bdd-tests/test-runner.js",
    "test:bdd:connection": "node scripts/bdd-tests/be001-1-connection.test.js",
    "test:bdd:presence": "node scripts/bdd-tests/be001-2-presence.test.js",
    "test:jwt": "node scripts/generate-test-jwt.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"*.json\" \"*.md\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" \"*.json\" \"*.md\"",
    "quality": "npm run format:check && npm run lint:check",
    "quality:fix": "npm run format && npm run lint",
    "analyze": "node scripts/analyze-complexity.js --type=all",
    "analyze:cognitive": "node scripts/analyze-complexity.js --type=cognitive",
    "analyze:cyclomatic": "node scripts/analyze-complexity.js --type=cyclomatic",
    "analyze:functions": "node scripts/analyze-complexity.js --type=functions",
    "analyze:security": "node scripts/analyze-complexity.js --type=security",
    "analyze:json": "node scripts/analyze-complexity.js --type=all --json",
    "analyze:report": "mkdir -p reports && node scripts/analyze-complexity.js --type=all --json > reports/complexity-report.json && echo 'Report saved to reports/complexity-report.json'",
    "security:check": "node scripts/check-secrets.js",
    "security:scan": "npm audit --audit-level=moderate",
    "verify": "npm run format:check && npm run lint:check && npm run test && npm run build",
    "verify:full": "npm run verify && npm run test:coverage:check",
    "verify:security": "npm run verify:full && npm run security:check && npm run security:scan",
    "ci": "npm run verify:full",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "release": "node scripts/auto-release.js --type=\"auto\"",
    "release:suggest": "node scripts/auto-release.js --type=\"auto\" --dry-run",
    "release:dry": "node scripts/auto-release.js --type=\"auto\" --dry-run",
    "release:minor": "node scripts/auto-release.js --type=\"minor\"",
    "release:major": "node scripts/auto-release.js --type=\"major\"",
    "release:patch": "node scripts/auto-release.js --type=\"patch\""
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.9",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.9",
    "@prisma/client": "^6.16.1",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "joi": "^17.13.3",
    "moment-timezone": "^0.6.0",
    "pg": "^8.16.3",
    "prisma": "^6.16.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.26",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.30.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@testcontainers/postgresql": "^11.5.1",
    "@types/babel__template": "^7.4.4",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.15.5",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "conventional-changelog": "^7.1.1",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-commits-parser": "^6.2.0",
    "dotenv": "^17.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.25.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.8",
    "socket.io-client": "^4.8.1",
    "source-map-support": "^0.5.21",
    "standard-version": "^9.5.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.8.0",
    "npm": ">=10.0.0"
  }
}
